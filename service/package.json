{
  "name": "rock-paper-scissors-lizard-spock",
  "version": "1.0.0",
  "description": "JS project for the rock paper scissors lizard spock game",
  "scripts": {
    "build": "npm run clean && npm run lint && npm run compile && npm test",
    "clean": "rm -rf build",
    "test": "JEST_REPORT_FILE=unit-test-results.json jest --config=.jestrc.json",
    "compile": "babel src --out-dir build --source-maps --copy-files",
    "lint": "eslint .",
    "lint-fix": "eslint . --fix"
  },
  "dependencies": {
    "winston": "^3.3.3",
    "@babel/runtime-corejs3": "^7.11.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.11.6",
    "@babel/core": "^7.11.6",
    "@babel/preset-env": "^7.11.5",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "babel-plugin-source-map-support": "^2.1.3",
    "aws-sdk": "2.751.0",
    "eslint": "^7.9.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-plugin-immutable": "^1.0.0",
    "eslint-plugin-import": "^2.22.0",
    "jest": "^26.4.2"
  },
  "keywords": [],
  "author": "andrewfitzy",
  "license": "GPLv3"
}
